{
  "name": "s3.vm.web",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker:stack": "docker stack deploy -c ./docker/api.yml vm_stack",
    "docker:stack:proxy": "docker stack deploy -c ./docker/api.yml -c ./docker/api-proxy.yml vm_stack",
    "docker:destroy": "docker stack rm vm_stack",
    "swagger:gen": "curl --insecure http://localhost:4302/swagger/v1/swagger.json --output swagger.json && node_modules/@openapitools/openapi-generator-cli/bin/openapi-generator generate -i ./swagger.json -g typescript-angular -o src/app/generated/vm-api --additional-properties ngVersion=9.1 --additional-properties useRxJS6=true --additional-properties modelPropertyNaming=original",
    "swagger:gen-win": "docker run --rm -v %CD%:/local openapitools/openapi-generator-cli:v4.1.2 generate -i http://host.docker.internal:4302/swagger/v1/swagger.json?format=openapi -g typescript-angular -o /local/src/app/generated/vm-api --additional-properties ngVersion=9.2 --additional-properties useRxJS6=true --additional-properties modelPropertyNaming=original"
  },
  "scriptComments": {
    "docker:stack": "// Runs the entire vm stack",
    "docker:stack:proxy": "// Runs the entire caster vm with proxy settings",
    "docker:destroy": "// Destroys the docker vm, data retained"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.11",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.11",
    "@angular/compiler": "^9.1.11",
    "@angular/core": "^9.1.11",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "^9.1.11",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.11",
    "@angular/platform-browser-dynamic": "^9.1.11",
    "@angular/router": "^9.1.11",
    "@crucible/common": "file:modules/crucible-common-0.0.3.tgz",
    "@datorama/akita": "^5.0.5",
    "@microsoft/signalr": "^3.1.5",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.6.5",
    "ngx-drag-to-select": "^3.1.1",
    "oidc-client": "^1.10.1",
    "rxjs": "^6.5.5",
    "string-natural-compare": "^3.0.1",
    "tslib": "^1.10.0",
    "typescript": "^3.8.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.9",
    "@angular/cli": "^9.1.9",
    "@angular/compiler-cli": "^9.1.11",
    "@angular/language-service": "^9.1.11",
    "@datorama/akita-ng-router-store": "^5.1.3",
    "@datorama/akita-ngdevtools": "^5.0.1",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.0.13",
    "codelyzer": "^5.2.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "~7.0.0",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.2",
    "ws": "7.3.0"
  }
}
